<Window x:Class="sample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="wsclient - test.app" Height="450" Width="800">
    <Grid>
        <Grid >
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            
            <Border Grid.Row="0" Background="DarkGray" CornerRadius="5" Margin="4" Padding="5" BorderThickness="1" BorderBrush="Black" >
                <WrapPanel Orientation="Vertical">
                    <Label>Rest API</Label>
                    <WrapPanel Orientation="Horizontal">
                        <Button Click="Button_Login" Padding="5 2" Width="80" Height="28" Margin="2">Login</Button>
                    </WrapPanel>
                </WrapPanel>
            </Border>
            <Border Grid.Row="2" Background="DarkGray" CornerRadius="5" Margin="4 0" Padding="5" BorderThickness="1" BorderBrush="Black" >
                <WrapPanel Orientation="Vertical">
                    <Label>Socket</Label>
                    <WrapPanel Orientation="Horizontal">
                        <Button Click="Button_Connect" Padding="5 2" Width="80" Height="28" Margin="2">Connect</Button>
                        <Button Click="Button_Disconnect" Padding="5 2" Width="80" Height="28" Margin="2">Disconnect</Button>
                        <Button Click="Button_NestJS_Test" Padding="5 2" Width="140" Height="28" Margin="2">NestJS Gateway Test</Button>
                        <Button Click="Button_NestJS_Test_Exception" Padding="5 2" Width="180" Height="28" Margin="2">NestJS Test Gateway Exception</Button>
                    </WrapPanel>
                </WrapPanel>
            </Border>
            <Border Grid.Row="3" Background="DarkGray" CornerRadius="5" Margin="4" Padding="5" BorderThickness="1" BorderBrush="Black" >
                <WrapPanel Orientation="Vertical">
                    <Label>(User Service) WS Rest Api </Label>
                    <WrapPanel Orientation="Horizontal">
                        <Button Click="Button_FuncA" Padding="5 2" Width="80" Height="28" Margin="2">FuncA</Button>
                        <Button Click="Button_FuncB" Padding="5 2" Width="80" Height="28" Margin="2">FuncB</Button>
                        <Button Click="Button_FuncC" Padding="5 2" Width="80" Height="28" Margin="2">FuncC</Button>
                        <Button Click="Button_FuncD" Padding="5 2" Width="80" Height="28" Margin="2">FuncD</Button>
                        <Button Click="Button_FuncE" Padding="5 2" Width="80" Height="28" Margin="2">FuncE</Button>
                        <Button Click="Button_emitEvents" Padding="5 2" Width="80" Height="28" Margin="2">Emit Events</Button>
                        <Button Click="Button_ChangeUser" Padding="5 2" Width="80" Height="28" Margin="2">Change User</Button>
                    </WrapPanel>
                    <Label>(User Service) WS Hub Api</Label>
                   
                    <WrapPanel Orientation="Horizontal">
                        <Label Foreground="white" Width="150">'onUpdateCredentials'</Label>
                        <Button Name="onUpdateCredentials_Subscribe"    Click="onUpdateCredentials" Padding="5 2" Width="80" Height="28" Margin="2">subscribe</Button>
                        <Button Name="onUpdateCredentials_Unsubscribe"  Click="onUpdateCredentials" Padding="5 2" Width="80" Height="28" Margin="2">unsubscribe</Button>
                        <Button Name="onUpdateCredentials_On"           Click="onUpdateCredentials" Padding="5 2" Width="80" Height="28" Margin="2">on(...)</Button>
                        <Button Name="onUpdateCredentials_Off"          Click="onUpdateCredentials" Padding="5 2" Width="80" Height="28" Margin="2">off</Button>
                    </WrapPanel>

                    <WrapPanel Orientation="Horizontal">
                        <Label Foreground="white" Width="150">'onUpdateCredentialsData'</Label>
                        <Button Name="onUpdateCredentialsData_Subscribe"    Click="onUpdateCredentialsData" Padding="5 2" Width="80" Height="28" Margin="2">subscribe</Button>
                        <Button Name="onUpdateCredentialsData_Unsubscribe"  Click="onUpdateCredentialsData" Padding="5 2" Width="80" Height="28" Margin="2">unsubscribe</Button>
                        <Button Name="onUpdateCredentialsData_On"           Click="onUpdateCredentialsData" Padding="5 2" Width="80" Height="28" Margin="2">on(...)</Button>
                        <Button Name="onUpdateCredentialsData_Off"          Click="onUpdateCredentialsData" Padding="5 2" Width="80" Height="28" Margin="2">off</Button>
                    </WrapPanel>

                    <WrapPanel Orientation="Horizontal">
                        <Label Foreground="white" Width="150">'onUpdateData'</Label>
                        <Button Name="onUpdateData_Subscribe"    Click="onUpdateData" Padding="5 2" Width="80" Height="28" Margin="2">subscribe</Button>
                        <Button Name="onUpdateData_Unsubscribe"  Click="onUpdateData" Padding="5 2" Width="80" Height="28" Margin="2">unsubscribe</Button>
                        <Button Name="onUpdateData_On"           Click="onUpdateData" Padding="5 2" Width="80" Height="28" Margin="2">on(...)</Button>
                        <Button Name="onUpdateData_Off"          Click="onUpdateData" Padding="5 2" Width="80" Height="28" Margin="2">off</Button>
                    </WrapPanel>

                    <WrapPanel Orientation="Horizontal">
                        <Label Foreground="white" Width="150">'onUpdate'</Label>
                        <Button Name="onUpdate_Subscribe"    Click="onUpdate" Padding="5 2" Width="80" Height="28" Margin="2">subscribe</Button>
                        <Button Name="onUpdate_Unsubscribe"  Click="onUpdate" Padding="5 2" Width="80" Height="28" Margin="2">unsubscribe</Button>
                        <Button Name="onUpdate_On"           Click="onUpdate" Padding="5 2" Width="80" Height="28" Margin="2">on(...)</Button>
                        <Button Name="onUpdate_Off"          Click="onUpdate" Padding="5 2" Width="80" Height="28" Margin="2">off</Button>
                    </WrapPanel>
                </WrapPanel>
            </Border>
        </Grid>
    </Grid>
</Window>
