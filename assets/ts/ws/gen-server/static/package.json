{
	"name": "template.api.server",
	"version": "1.0.0",
	"license": "MIT",
	"main": "dist/main.js",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "cross-env NODE_ENV=production nest build",
		"start": "cross-env NODE_ENV=development nest start",
		"start:dev": "cross-env NODE_ENV=development nest start --watch",
		"start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
		"start:prod": "cross-env NODE_ENV=production node dist/main",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"wsclient:ts": "npx nex ts ws gen-client --install --build",
		"wsclient:cs": "npx nex cs ws gen-client --build",
		"httpclient:ts": "openapi-generator generate -g typescript-axios -i ./dist/swagger-spec.json --skip-validate-spec --enable-post-process-file -o ../template.api.httpclient-ts -c openapi.ts.config.json",
		"httpclient:cs": "openapi-generator generate -g csharp           -i ./dist/swagger-spec.json --skip-validate-spec --enable-post-process-file -o ../template.api.httpclient-cs -c openapi.cs.config.json",
		"doc:angular": "npx compodoc -p tsconfig.json -s -d documentation/angular",
		"doc:typedoc": "npx typedoc --out documentation/typedoc src --mode modules",
		"doc:serve-angular": "serve documentation/angular",
		"doc:serve-typedoc": "serve documentation/typedoc"
	},
	"dependencies": {
		"@nestjs/common": "^6.10.14",
		"@nestjs/core": "^6.10.14",
		"@nestjs/platform-express": "^6.10.14",
		"@nestjs/platform-socket.io": "^6.10.14",
		"@nestjs/serve-static": "^1.0.2",
		"@nestjs/swagger": "^4.1.7",
		"@nestjs/websockets": "^6.10.14",
		"@types/mongodb": "^3.3.14",
		"agenda": "^3.0.0",
		"agendash": "^1.0.0",
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.2.3",
		"class-validator": "^0.11.0",
		"dotenv-flow": "^3.1.0",
		"express-session": "^1.17.0",
		"helmet": "^3.21.2",
		"jsonwebtoken": "^8.5.1",
		"module-alias": "^2.2.2",
		"moment": "^2.24.0",
		"mongodb": "^3.5.1",
		"morgan": "^1.9.1",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^6.5.4",
		"strongly-typed-events": "^1.6.4",
		"swagger-ui-dist": "^3.24.3",
		"swagger-ui-express": "^4.1.2",
		"typedoc": "^0.16.9"
	},
	"devDependencies": {
		"@nestjs/cli": "^6.13.2",
		"@compodoc/compodoc": "^1.1.11",
		"@nexjs/cli": "^1.1.0",
		"@openapitools/openapi-generator-cli": "^1.0.8-4.2.2",
		"@types/agenda": "^2.0.6",
		"@types/bcrypt": "^3.0.0",
		"@types/bcryptjs": "^2.4.2",
		"@types/express": "^4.17.2",
		"@types/express-session": "^1.15.16",
		"@types/jsonwebtoken": "^8.3.8",
		"@types/lodash": "^4.14.149",
		"@types/module-alias": "^2.0.0",
		"@types/multer": "^1.3.10",
		"@types/node": "11.13.4",
		"@types/socket.io": "^2.1.4",
		"@types/uuid": "^3.4.6",
		"cross-env": "^6.0.3",
		"rimraf": "^3.0.0",
		"serve": "^11.3.0",
		"ts-loader": "^6.2.1",
		"ts-node": "^8.6.0",
		"tsc-watch": "^4.0.0",
		"tslint": "^5.20.1",
		"typescript": "^3.7.4"
	},
	"_moduleAliases": {
		"@": "./dist"
	}
}