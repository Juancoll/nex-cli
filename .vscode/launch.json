    "version": "0.2.0",
    "configurations": [
        // --------------------
        // TS WS Commands
        //  -------------------
        /* 
         * ws client     
         */
        {
            "type": "node",
            "request": "launch",
            "name": "nex ts ws gen-client",
            "program": "${workspaceFolder}/dist/cli.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "ts",
                "ws",
                "gen-client",
                "-s",
                "C:/git/nodall-training/template.api.server",
                "-o",
                "c:/wsclient",
                "-n",
                "aaaa",
                "-i",
                "-b"
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "nex ts ws up-client",
            "program": "${workspaceFolder}/dist/cli.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "ts",
                "ws",
                "up-client",
                "-s",
                "C:/git/nodall-training/template.api.server",
                "-o",
                "c:/wsclient/src",
                "-b"
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "------------------"
        },
        /* 
         * ws demo     
         */
        {
            "type": "node",
            "request": "launch",
            "name": "nex ts ws gen-demo",
            "program": "${workspaceFolder}/dist/cli.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "ts",
                "ws",
                "gen-demo",
                "-s",
                "C:/git/nodall-training/template.api.server",
                "-o",
                "c:/wsdemo",
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "nex ts ws up-demo",
            "program": "${workspaceFolder}/dist/cli.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "ts",
                "ws",
                "up-demo",
                "-s",
                "C:/git/nodall-training/template.api.server",
                "-o",
                "c:/wsdemo",
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "------------------"
        },
        // --------------------
        // CS WS Commands
        //  -------------------
        {
            "type": "node",
            "request": "launch",
            "name": "nex cs ws up-client",
            "program": "${workspaceFolder}/dist/cli.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "cs",
                "ws",
                "gen-client",
                "--build"
            ],
            "env": {
                "NODE_ENV": "debug"
            }
        }
    ]
}