    "version": "0.2.0",
    "configurations": [
        // --------------------
        // TS WS Commands
        //  -------------------
        /* 
         * ws client     
         */
        {
            "type": "node",
            "request": "launch",
            "name": "ts ws gen-client",
            "program": "${workspaceFolder}/dist/cli.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "ts",
                "ws",
                "gen-client",
                "-s",
                "C:/git/nodall-training/template.api.server",
                "-o",
                "c:/wsclient.api.ts",
                "-i",
                "-b"
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "ts ws up-client",
            "program": "${workspaceFolder}/dist/cli.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "ts",
                "ws",
                "up-client",
                "-s",
                "C:/git/nodall-training/template.api.server",
                "-o",
                "c:/wsclient.api.ts/src",
                "-b"
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "------------------"
        },
        /* 
         * ws demo     
         */
        {
            "type": "node",
            "request": "launch",
